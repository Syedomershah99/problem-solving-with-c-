#include <iostream>
using namespace std;

int main() {
    char ch;
    cin >> ch;

    if (ch == 'V') {
        // Generating the V pattern
        for (int i = 1; i <= 5; ++i) {
            for (int j = 1; j <= i; ++j) {
                cout << j;
            }
            for (int j = 1; j <= 2 * (5 - i); ++j) {
                cout << " ";
            }
            for (int j = i; j >= 1; --j) {
                cout << j;
            }
            cout << endl;
        }
    } else {
        cout << "Invalid input. Please enter 'V'." << endl;
    }

    return 0;
}
