#include<iostream>
using namespace std;

int main() {
    int T;
    cin >> T;

    while (T--) {
        int N;
        cin >> N;

        // Check if it's a leap year
        if ((N % 4 == 0 && N % 100 != 0) || (N % 400 == 0)) {
            cout << "Yes" << endl;
        } else {
            cout << "No" << endl;
        }
    }

    return 0;
}
