#include <bits/stdc++.h>
  using namespace std;
  
  int main()
  {
    int t,i = 0,n;
    cin>>t;
    while(t--)
    {
      cin>>n;
      if(n%10==0)
      {
        cout<<0<<endl;
      }
      else if(n%5==0||n==5)
      {
        cout<<1<<endl;
      }
      else{
        cout<<-1<<endl;
      }
    }
    return 0;
  }
